{
  "family": "mi-task-fargate",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::472188560076:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "mi-nestjs-app",
      "image": "REPLACE_IMAGE",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
    
  ]
  
}
